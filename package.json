    "tiny-script-loader": "^2.2.1",
    "vue": "^3.5.13",
    "vue-select": "^4.0.0-beta.6",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.315.0",
