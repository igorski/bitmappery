{
  "name": "bitmappery",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "server": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "test": "vitest --environment jsdom --no-watch",
    "typecheck": "npx -p typescript tsc -p tsconfig.check.json --noEmit --resolveJsonModule",
