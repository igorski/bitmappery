    "lz-string": "^1.4.4",
    "pdfjs-dist": "^3.5.141",
    "sass": "^1.51.0",
    "typescript": "~4.7.2",
    "undo-manager": "^1.0.5",
    "vite": "^4.1.4",
    "vite-plugin-static-copy": "^0.13.1",
    "vitest": "^0.29.3",
    "vitest-canvas-mock": "^0.2.2",
    "vue-i18n": "^8.22.2",
    "vue-js-toggle-button": "^1.3.3",
    "vue-select": "^3.6.0",
    "vuedraggable": "^2.24.3",
    "zcanvas": "^5.1.8"
  }
